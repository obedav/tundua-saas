{
    "name": "tundua/saas-backend",
    "description": "Tundua Study Abroad SaaS Platform - Backend API",
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": ">=8.0",
        "dompdf/dompdf": "^2.0",
        "firebase/php-jwt": "^6.9",
        "guzzlehttp/guzzle": "^7.8",
        "illuminate/database": "^10.0",
        "intervention/image": "^2.7",
        "league/flysystem": "^3.0",
        "league/oauth2-google": "^4.0",
        "monolog/monolog": "^3.5",
        "php-di/php-di": "^7.0",
        "phpmailer/phpmailer": "^6.9",
        "ramsey/uuid": "^4.7",
        "respect/validation": "^2.3",
        "slim/psr7": "^1.6",
        "slim/slim": "^4.12",
        "stripe/stripe-php": "^13.0",
        "vlucas/phpdotenv": "^5.6",
        "yabacon/paystack-php": "^2.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5",
        "fakerphp/faker": "^1.23"
    },
    "autoload": {
        "psr-4": {
            "Tundua\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tundua\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "start": "php -S localhost:8000 -t public",
        "test": "phpunit"
    },
    "config": {
        "process-timeout": 0,
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
